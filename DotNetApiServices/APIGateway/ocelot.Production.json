{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/Auth/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "Key": "AuthService",
      "SwaggerKey": "AuthService"
    },
    {
      "DownstreamPathTemplate": "/api/Customer/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "customerservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/Customer/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "Key": "CustomerService",
      "SwaggerKey": "CustomerService",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/api/Account/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "accountservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/Account/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "Key": "AccountService",
      "SwaggerKey": "AccountService",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    },
    {
      "DownstreamPathTemplate": "/api/Transaction/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "transactionservice",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/Transaction/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "Key": "TransactionService",
      "SwaggerKey": "TransactionService",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      }
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "AuthService",
      "Config": [
        {
          "Name": "Auth Service API V1",
          "Version": "v1",
          "Url": "http://authservice/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "CustomerService",
      "Config": [
        {
