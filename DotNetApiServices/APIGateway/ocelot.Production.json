      "Config": [
        {
          "Name": "Account Service API V1",
          "Version": "v1",
          "Url": "http://accountservice/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "TransactionService",
      "Config": [
        {
          "Name": "Transaction Service API V1",
          "Version": "v1",
          "Url": "http://transactionservice/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost"
  }
